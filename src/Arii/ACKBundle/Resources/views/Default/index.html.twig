{% extends "AriiACKBundle::layout.html.twig" %}
{% block body %}
<script language="javascript">
function Init() {
//    myLayout = globalLayout.cells("a").attachLayout('2U');        
    myJobs =  globalLayout.cells("c").attachChart({
            view:"donut",
            value:"#NB#",
            label:"",
            tooltip: "#NAME#",
            color: "#COLOR#",
            border:false,
            pieInnerText: "#NB#",
            shadow: 0,
            legend:{
                width: 0,
                template: "#NAME#",
                valign:"top",
                align:"left"
            } 
    });
    myJobs.load( "{{ url('xml_ACK_network_pie') }}" );
}
</script>
{% endblock %}
