{% extends "AriiCoreBundle::base.html.twig" %}
{% block head %}
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.min.css') }}">
<!-- Optional theme -->
<link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap-theme.min.css') }}">
<!-- Latest compiled and minified JavaScript -->
<script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
<script src="{{ asset('bootstrap/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/ariiats/js/jquery.tablesorter.min.js') }}"></script>
<link rel="stylesheet" type="text/css" href="{{ asset('dhtmlx/skins/terrace/dhtmlx.css') }}" />
<script src="{{ asset('dhtmlx/codebase/dhtmlx.js') }}" type="text/javascript"></script>
<script>  
var data = "\
			20, 02\n\
			55, 03\n\
			40, 04\n\
			78, 05\n\
			61, 06\n\
			35, 07\n\
			80, 08\n\
			50, 09\n\
			65, 10\n\
			59, 11";
		var myBarChart;
		function doOnLoad() {
			myBarChart = new dhtmlXChart({
				view:"bar",
				container:"chartDiv",
				value:"#data0#",
				radius:0,
				border:true,
				xAxis:{
					template:"'#data1#"
				},
				yAxis:{
					start:0,
					end:100,
					step:10,
					template:function(obj){
						return (obj%20?"":obj)
					}
				}
			});
			myBarChart.parse(data,"csv");
		}
</script>
{% endblock %} 
{% block body %}
<body onload="doOnLoad();">
<div class="container-fluid">
<div class="jumbotron">
<img style="float: right;" src="{{ asset('logo.png') }}"/>
  <h1>TEST</h1>
  <p>{{ url('xml_ATS_alarms_pie') }}</p>
  <div id="chartDiv" style="width:600px;height:250px;margin:20px;border:1px solid #c0c0c0"></div>
</div>
</body>
{% endblock %}
