<?xml version="1.0" encoding="utf-8" ?>
<toolbar>
{% if refresh>0 %}
    <item type="buttonSelect" id="refresh" text="" img="refresh.png" imgdis="filter.png">
        <item type="button" id="REFRESH_99999" text="{{ "Never" | trans }}"/>
        <item id="sep31" type="separator"/>    
        <item type="button" id="REFRESH_5" text="5 {{ "seconds" | trans }}" />
        <item type="button" id="REFRESH_30" text="30 {{ "seconds" | trans }}"/>
        <item type="button" id="REFRESH_60" text="1 {{ "minute" | trans }}" />
        <item type="button" id="REFRESH_300" text="5 {{ "minutes" | trans }}"/>
        <item type="button" id="REFRESH_900" text="15 {{ "minutes" | trans }}"/>
        <item type="button" id="REFRESH_3600" text="1 {{ "hour" | trans }}"/>
    </item>
{% endif %}
{% if export>0 %}
<item type="sep_export"/>
<item type="button" id="EXPORT_xls" text="Excel" img="xls.png"/>
<item type="button" id="EXPORT_pdf" text="Pdf" img="pdf.png"/>
{% endif %}
</toolbar>