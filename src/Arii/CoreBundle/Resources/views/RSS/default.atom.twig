<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
        <title>{{ "Job failures" | trans }}</title>
        <subtitle>{{ "Last jobs failed" | trans }}</subtitle>
        <link href="{{ url( 'arii_JID_history' ) }}"/>
        <updated>{{ "now" |date('Y-m-d H:i:s') }}</updated>
        {% for info in Infos %}
        <entry>
            <title>{{ info.end}} [{{ info.spooler }}]/{{ info.name }} (exit {{ info.exit }})</title>
            <id>{{ info.id }}</id>
            <summary>{{ info.status }}</summary>
            <updated>{{ info.end}}</updated>
            <category>{{ info.spooler }}</category>
            <link/>
        </entry>
        {% endfor %}
</feed>